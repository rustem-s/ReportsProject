        <Field Name="others_diff">
          <DataField>others_diff</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DS_VLR</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select
	t.accessing_date,
	win.count win,
	win.count - win_prev_day.count win_diff,
	mtsr.count mtsr,
	mtsr.count - mtsr_prev_day.count mtsr_diff,
	mtsr_new.count - mtsr_new_prev_day.count mtsr_new_diff,
	others.count others,
	others.count - others_prev_day.count others_diff
from
(
	select distinct accessing_date
	from AGGR_PREFIX_BY_DAY
) t 

left join
(
	select accessing_date, sum(msisdn_count) count
	from AGGR_PREFIX_BY_DAY
	where msisdn_prefix in ('79789')
	group by accessing_date
) win
on t.ACCESSING_DATE = win.ACCESSING_DATE

left join
(
	select accessing_date, sum(msisdn_count) count
	from AGGR_PREFIX_BY_DAY
	where msisdn_prefix in ('79789')
	group by accessing_date
) win_prev_day
on DATEADD(DAY, -1, t.ACCESSING_DATE) = win_prev_day.ACCESSING_DATE

left join
(
	select accessing_date, sum(msisdn_count) count
	from AGGR_PREFIX_BY_DAY
	where msisdn_prefix in ('79780', '79787', '79788')
	group by accessing_date
) mtsr
on t.ACCESSING_DATE = mtsr.ACCESSING_DATE

left join
(
	select accessing_date, sum(msisdn_count) count
	from AGGR_PREFIX_BY_DAY
	where msisdn_prefix in ('79780', '79787', '79788')
	group by accessing_date
) mtsr_prev_day
on DATEADD(DAY, -1, t.ACCESSING_DATE) = mtsr_prev_day.ACCESSING_DATE

left join
(
	select accessing_date, sum(msisdn_count) count
	from AGGR_PREFIX_BY_DAY
	where msisdn_prefix in ('79781')
	group by accessing_date
) mtsr_new
on t.ACCESSING_DATE = mtsr_new.ACCESSING_DATE

left join
(
	select accessing_date, sum(msisdn_count) count
	from AGGR_PREFIX_BY_DAY
	where msisdn_prefix in ('79781')
	group by accessing_date
) mtsr_new_prev_day
on DATEADD(DAY, -1, t.ACCESSING_DATE) = mtsr_new_prev_day.ACCESSING_DATE

left join
(
	select accessing_date, sum(msisdn_count) count
	from AGGR_PREFIX_BY_DAY
	where msisdn_prefix not in ('79780', '79781', '79787', '79788', '79789')
	group by accessing_date
) others
on t.ACCESSING_DATE = others.ACCESSING_DATE

left join
(
	select accessing_date, sum(msisdn_count) count
	from AGGR_PREFIX_BY_DAY
	where msisdn_prefix not in ('79780', '79781', '79787', '79788', '79789')
	group by accessing_date
) others_prev_day
on DATEADD(DAY, -1, t.ACCESSING_DATE) = others_prev_day.ACCESSING_DATE

order by t.ACCESSING_DATE</CommandText>
