  <DataSets>
    <DataSet Name="DSMTS">
      <Query>
        <DataSourceName>DS_VLR</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PMonth">
            <Value>=Parameters!PMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PYear">
            <Value>=Parameters!PYear.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>	with 
	mts(accessing_date, operator_name, quantity) as
	(
		select 
			rep.accessing_date,
			o.name operator_name,
			rep.msisdn_count quantity
		from 
			rep_01_day rep,
			ref_operator o
		where
			rep.id_ref_operator = 2
			and rep.id_ref_operator = o.id
	),

	mts_new(accessing_date, operator_name, quantity) as
	(
		select 
			rep.accessing_date,
			o.name operator_name,
			rep.msisdn_count quantity
		from 
			rep_01_day rep,
			ref_operator o
		where
			rep.id_ref_operator = 3
			and rep.id_ref_operator = o.id
	),

	mts_adds(accessing_date, operator_name, quantity) as
	(
		select 
			rep.accessing_date,
			o.name operator_name,
			rep.msisdn_adds_count quantity
		from 
			rep_01_day rep,
			ref_operator o
		where
			rep.id_ref_operator = 2
			and rep.id_ref_operator = o.id
	),

	mts_new_adds(accessing_date, operator_name, quantity) as
	(
		select 
			rep.accessing_date,
			o.name operator_name,
			rep.msisdn_adds_count quantity
		from 
			rep_01_day rep,
			ref_operator o
		where
			rep.id_ref_operator = 3
			and rep.id_ref_operator = o.id
	)

	-- mts
	select
		1 sort,
		accessing_date,
		operator_name,
		quantity
	from mts
	where
		month(accessing_date) = @PMonth and year(accessing_date) = @PYear
	union all
	-- mts_new
	select
		2 sort,
		accessing_date,
		operator_name,
		quantity
	from mts_new
	where
		month(accessing_date) = @PMonth and year(accessing_date) = @PYear
	union all
	-- total (mts + mts_new)
	select
		3 sort,
		mts.accessing_date,
		'Total' operator_name,
		mts.quantity + mts_new.quantity quantity
	from mts, mts_new
	where
		mts.accessing_date = mts_new.accessing_date
		and month(mts.accessing_date) = @PMonth and year(mts.accessing_date) = @PYear
	union all
	-- mts adds
	select
		4 sort,
		accessing_date,
		operator_name + ' activations',
		quantity
	from mts_adds
	where
		month(accessing_date) = @PMonth and year(accessing_date) = @PYear
	union all
	-- mts_new_adds
	select
		5 sort,
		accessing_date,
		operator_name + ' activations',
		quantity
	from mts_new_adds
	where
		month(accessing_date) = @PMonth and year(accessing_date) = @PYear
	union all
	-- total (mts_adds + mts_new_adds)
	select
		6 sort,
		mts_adds.accessing_date,
		'Total' operator_name,
		mts_adds.quantity + mts_new_adds.quantity quantity
	from mts_adds, mts_new_adds
	where
		mts_adds.accessing_date = mts_new_adds.accessing_date
		and month(mts_adds.accessing_date) = @PMonth and year(mts_adds.accessing_date) = @PYear</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
